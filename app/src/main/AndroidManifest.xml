        <activity android:name=".NotFinishedEvent">
        </activity>
        <activity android:name=".AddDriversActivity">
        </activity>
        <activity android:name=".NotFinishedEventDetail">
        </activity>
        <activity android:name=".ListDriverDetailActivity">
        </activity>
    </application>

</manifest>